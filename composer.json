{
    "name": "phant/assets-versions",
    "description": "get assets versions easily",
    "license": "MIT",
    "keywords": ["assets", "versions", "assets versions", "git assets versions", "assets cache auto-revocation"],
    "authors": [
        {
            "name": "Lenny ROUANET",
            "email": "lennyrouanet@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "phant/cache": "1.*",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse component --memory-limit=4G"
    },
    "autoload": {
        "psr-4": {
            "Phant\\AssetsVersions\\": "component/"
        }
    }
}
